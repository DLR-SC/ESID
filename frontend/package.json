{
  "name": "esid",
  "version": "0.0.1",
  "description": "Visualization of infectious disease propagation.",
  "scripts": {
    "build": "npm run collect-attributions && webpack --progress --config ./webpack/webpack.prod.js",
    "start": "webpack serve --open --progress --config ./webpack/webpack.dev.js",
    "start-prod": "webpack serve --open --progress --config ./webpack/webpack.prod.js",
    "test": "jest",
    "coverage": "jest --collectCoverage --reporters='default'",
    "format": "prettier --write .",
    "lint": "eslint --fix src",
    "collect-attributions": "ts-node ./tools/CollectAttributions.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DLR-SC/ESID.git"
  },
  "author": "German Aerospace Center (DLR)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/DLR-SC/ESID/issues"
  },
  "homepage": "https://github.com/DLR-SC/ESID#readme",
  "sideEffects": false,
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "reports",
          "outputName": "jest-junit.xml"
        }
      ]
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**",
      "!src/main.tsx"
    ],
    "coverageDirectory": "reports"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.17",
    "@amcharts/amcharts5": "^5.1.8",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "*",
    "@mui/material": "^5.10.11",
    "@mui/styles": "^5.10.10",
    "@mui/system": "^5.10.10",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/geojson": "^7946.0.8",
    "i18next": "^20.2.4",
    "i18next-browser-languagedetector": "^6.1.1",
    "i18next-http-backend": "^1.2.4",
    "json5": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.15",
    "react-lazyload": "^3.2.0",
    "react-markdown": "^6.0.3",
    "react-redux": "^7.2.3",
    "react-scroll-sync": "^0.11.0"
    
   
    
    
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.13",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.0",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-lazyload": "^3.1.1",
    "@types/react-redux": "^7.1.18",
    "@types/react-scroll-sync": "^0.8.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.1.3",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.3",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.5",
    "jest-junit": "^13.0.0",
    "lint-staged": "^10.5.4",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.1",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.0.0",
    "typescript": "^4.8.4 <4.9.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.7.3"
  }
}
