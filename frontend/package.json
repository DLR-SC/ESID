    "country-flag-icons": "^1.5.9",
    "i18next": "^23.7.10",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "json5": "^2.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-lazyload": "github:twobin/react-lazyload",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.0.4",
    "react-scroll-sync": "^0.11.2",
    "redux": "^5.0.0",
    "redux-persist": "^6.0.0",
    "rooks": "^7.14.1"
