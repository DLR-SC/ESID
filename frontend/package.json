    "make-pretty": "prettier --write .",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DLR-SC/ESID.git"
  },
  "author": "German Aerospace Center (DLR)",
  "license": "UNLICENSED",
  "bugs": {
